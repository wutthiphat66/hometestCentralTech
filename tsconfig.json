{
  "extends": "./tsconfig.json",
  "include": [
    "./**/*.ts",
    "integration/*.ts",
    "support/*.ts",
    "../node_modules/cypress"
  ],
  "exclude": [],
  "compilerOptions": {
    "types": ["cypress", "cypress/types"],
    "lib": ["es2017", "dom"],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true
  }
}